{
    "name": "sajid/unit-conversions",
    "description": "php unit conversions",
    "keywords": [
        "sajid",
        "unit-conversions"
    ],
    "homepage": "https://github.com/SajidSadeeq/unit-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "sajid",
            "email": "author@domain.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "9.5",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Sajid\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sajid\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
